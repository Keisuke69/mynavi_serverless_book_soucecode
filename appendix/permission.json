{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "logs:*",
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:ListStreams",
                "dynamodb:GetRecords",
                "dynamodb:GetShardIterator",
                "dynamodb:DescribeStream"
            ],
            "Resource": [
                "arn:aws:dynamodb:ap-northeast-1:177352676277:table/photos"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObject",
            ],
            "Resource": [
                "arn:aws:s3:::serverless-app-photos-177352676277/*"
            ]
        }
    ]
}
